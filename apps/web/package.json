  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --port 3001",
    "build": "next build",
    "start": "next start",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:open": "playwright test --headed --project=chromium",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
